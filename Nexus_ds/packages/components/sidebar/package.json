{
    "name": "@nexus-ds/sidebar",
    "version": "0.0.1",
    "description": "Chips help people enter information, make selections, filter content, or trigger actions.",
    "keywords": [
      "sidebar"
    ],
    
    "main": "src/index.ts",
    "sideEffects": false,
    "files": [
      "dist"
    ],
    "publishConfig": {
      "access": "public"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/NexusDesignSystem/nexus-ds.git",
      "directory": "packages/components/chip"
    },
    "bugs": {
      "url": "https://github.com/NexusDesignSystem/nexus-ds/issues"
    },
    "scripts": {
      "build": "tsup src --dts",
      "build:fast": "tsup src",
      "dev": "yarn build:fast -- --watch",
      "clean": "rimraf dist .turbo",
      "typecheck": "tsc --noEmit",
      "prepack": "clean-package",
      "postpack": "clean-package restore"
    },
    "peerDependencies": {
      "react": ">=18",
      "react-dom": ">=18"
    },
    "dependencies": {
      "@nexus-ds/shared-icons": "workspace:*",
      "@nexus-ds/shared-utils": "workspace:*",
      "@nexus-ds/react-utils": "workspace:*",
      "@nexus-ds/system": "workspace:*",
      "@nexus-ds/theme": "workspace:*",
      "@nexus-ds/dropdown": "workspace:*",
      "@nexus-ds/button": "workspace:*",
      "@nexus-ds/menu": "workspace:*",
      "@react-aria/focus": "^3.14.1",
      "@react-aria/interactions": "^3.18.0",
      "@react-aria/utils": "^3.20.0",
      "@react-types/checkbox": "^3.5.1"
    },
    "devDependencies": {
      "@nexus-ds/avatar": "workspace:*",
      "clean-package": "2.2.0",
      "react": "^18.0.0"
    },
    "clean-package": "../../../clean-package.config.json"
  }
  