{
  "name": "@nexus-ds/navbar",
  "version": "1.1.0",
  "description": "A responsive navigation header positioned on top side of your page that includes support for branding, links, navigation, collapse and more.",
  "keywords": [
    "navbar"
  ],
  
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NexusDesignSystem/nexus-ds.git",
    "directory": "packages/components/navbar"
  },
  "bugs": {
    "url": "https://github.com/NexusDesignSystem/nexus-ds/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "framer-motion": ">=4.0.0"
  },
  "dependencies": {
    "@nexus-ds/shared-utils": "workspace:*",
    "@nexus-ds/react-utils": "workspace:*",
    "@nexus-ds/system": "workspace:*",
    "@nexus-ds/theme": "workspace:*",
    "@nexus-ds/framer-transitions": "workspace:*",
    "@nexus-ds/use-aria-toggle-button": "workspace:*",
    "@nexus-ds/use-scroll-position": "workspace:*",
    "@react-aria/focus": "^3.14.1",
    "@react-aria/interactions": "^3.18.0",
    "@react-aria/utils": "^3.20.0",
    "@react-stately/toggle": "^3.6.2",
    "@react-stately/utils": "^3.7.0",
    "@react-aria/overlays": "^3.17.0",
    "react-remove-scroll": "^2.5.6"
  },
  "devDependencies": {
    "@nexus-ds/avatar": "workspace:*",
    "@nexus-ds/button": "workspace:*",
    "@nexus-ds/dropdown": "workspace:*",
    "@nexus-ds/input": "workspace:*",
    "@nexus-ds/link": "workspace:*",
    "@nexus-ds/shared-icons": "workspace:*",
    "clean-package": "2.2.0",
    "react": "^18.0.0",
    "react-lorem-component": "0.13.0"
  },
  "clean-package": "../../../clean-package.config.json"
}
