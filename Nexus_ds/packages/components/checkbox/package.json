{
  "name": "@nexus-ds/checkbox",
  "version": "1.1.0",
  "description": "Checkboxes allow users to select multiple items from a list of individual items, or to mark one individual item as selected.",
  "keywords": [
    "checkbox"
  ],
  
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NexusDesignSystem/nexus-ds.git",
    "directory": "packages/components/checkbox"
  },
  "bugs": {
    "url": "https://github.com/NexusDesignSystem/nexus-ds/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@nexus-ds/system": "workspace:*",
    "@nexus-ds/shared-utils": "workspace:*",
    "@nexus-ds/react-utils": "workspace:*",
    "@nexus-ds/theme": "workspace:*",
    "@react-aria/checkbox": "^3.11.0",
    "@react-aria/focus": "^3.14.1",
    "@react-aria/interactions": "^3.18.0",
    "@react-aria/visually-hidden": "^3.8.4",
    "@react-stately/checkbox": "^3.5.0",
    "@react-stately/toggle": "^3.6.2",
    "@react-aria/utils": "^3.20.0",
    "@react-types/checkbox": "^3.5.1",
    "@react-types/shared": "^3.20.0"
  },
  "devDependencies": {
    "@nexus-ds/shared-icons": "workspace:*",
    "@nexus-ds/chip": "workspace:*",
    "@nexus-ds/user": "workspace:*",
    "@nexus-ds/link": "workspace:*",
    "clean-package": "2.2.0",
    "react": "^18.0.0"
  },
  "clean-package": "../../../clean-package.config.json"
}
