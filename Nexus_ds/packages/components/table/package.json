{
  "name": "@nexus-ds/table",
  "version": "1.1.0",
  "description": "Tables are used to display tabular data using rows and columns. ",
  "keywords": [
    "table"
  ],
  
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NexusDesignSystem/nexus-ds.git",
    "directory": "packages/components/table"
  },
  "bugs": {
    "url": "https://github.com/NexusDesignSystem/nexus-ds/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@nexus-ds/checkbox": "workspace:*",
    "@nexus-ds/react-utils": "workspace:*",
    "@nexus-ds/shared-icons": "workspace:*",
    "@nexus-ds/shared-utils": "workspace:*",
    "@nexus-ds/spacer": "workspace:*",
    "@nexus-ds/system": "workspace:*",
    "@nexus-ds/theme": "workspace:*",
    "@react-aria/focus": "^3.14.1",
    "@react-aria/interactions": "^3.18.0",
    "@react-aria/table": "^3.12.0",
    "@react-aria/utils": "^3.20.0",
    "@react-aria/visually-hidden": "^3.8.4",
    "@react-stately/table": "^3.11.1",
    "@react-stately/virtualizer": "^3.6.2",
    "@react-types/grid": "^3.2.1",
    "@react-types/table": "^3.8.1"
  },
  "devDependencies": {
    "@nexus-ds/button": "workspace:*",
    "@nexus-ds/chip": "workspace:*",
    "@nexus-ds/pagination": "workspace:*",
    "@nexus-ds/spinner": "workspace:*",
    "@nexus-ds/tooltip": "workspace:*",
    "@nexus-ds/use-infinite-scroll": "workspace:*",
    "@nexus-ds/user": "workspace:*",
    "@react-stately/data": "^3.10.2",
    "clean-package": "2.2.0",
    "react": "^18.0.0",
    "swr": "^2.2.1"
  },
  "clean-package": "../../../clean-package.config.json"
}
