{
  "name": "@nexus-ds/system-rsc",
  "version": "1.1.0",
  "description": "nexusui system primitives compatibles with RSC imports",
  "keywords": [
    "system-rsc"
  ],
  
  "main": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NexusDesignSystem/nexus-ds.git",
    "directory": "packages/core/system-rsc"
  },
  "bugs": {
    "url": "https://github.com/NexusDesignSystem/nexus-ds/issues"
  },
  "scripts": {
    "build": "tsup src --dts",
    "build:fast": "tsup src",
    "dev": "yarn build:fast -- --watch",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "@nexus-ds/theme": "workspace:*",
    "@nexus-ds/react-utils": "workspace:*",
    "clean-package": "2.2.0"
  },
  "dependencies": {
    "tailwind-variants": "^0.1.14",
    "clsx": "^1.2.1"
  },
  "clean-package": "../../../clean-package.config.json",
  "tsup": {
    "clean": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ]
  }
}
